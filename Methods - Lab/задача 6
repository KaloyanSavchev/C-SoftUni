using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp5
{
    class Program
    {
        static void Main()

        {
            double num = double.Parse(Console.ReadLine());
            int power = int.Parse(Console.ReadLine());
            Console.WriteLine(loopOfString(num, power));                        
        }   
         static double loopOfString(double num, int power)
        {
            double result = 1;
            for (int i = 0; i < power; i++)
            
                result *= num ;
               return result;                      
        }                
    }
}
