using System;

namespace P04_Elevator
{
    class P04_Elevator
    {
        static void Main(string[] args)
        {
		int allPeople = int.Parse(Console.ReadLine());
		int capacity = int.Parse(Console.ReadLine());
        int allCourse = (int)Math.Ceiling((double)allPeople / capacity);
		Console.WriteLine(allCourse);
		}
	}
}	
