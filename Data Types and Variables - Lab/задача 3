using System;
					
public class Program
{
	public static void Main()
	{
		int numbers = int.Parse(Console.ReadLine());
		decimal final = 0;
		for(int i = 0; i < numbers; i++ )
		   
		{		
			decimal newNum = decimal.Parse(Console.ReadLine());
			final += newNum;
			
		}
		Console.WriteLine(final);
	}
}
