using System;
using System.Numerics;
using System.Collections.Generic;
using System.Linq;
using System.Text.RegularExpressions;
namespace zadacha3
{
    class Program
    {
        static void Main(string[] args)
        {
            int num = int.Parse(Console.ReadLine());
            BigInteger result = 1;
            for (int i = 0; i < num; i++)
            {
                result *= 2;
            }
            Console.WriteLine(result);
        }
    }
}
